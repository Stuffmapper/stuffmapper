#tab-container
	ul.stuff-tabs
		li.logo-tab
			a(ng-click)
		li.get-stuff-tab
			a.tk-proxima-nova(ui-sref="stuff.get") Get Stuff
		li.give-stuff-tab
			a.tk-proxima-nova(ng-click="toGiveStuff()") Give Stuff
		li.my-stuff-tab
