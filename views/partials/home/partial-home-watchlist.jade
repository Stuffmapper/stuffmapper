.watchlist-container
	div(ui-view)
	section.tag-container
			ul.tag-list
				li(ng-repeat='tagname in tagnames') {{tagname.tagname}}
	.tag-form(role='form')
		input(type='text', ng-model='tagname', placeholder='-->> Enter Tagname Here <<--')
		button(type='submit', value='Submit', ng-click='addTag()') Submit
